[user]
[commit]
	gpgsign = true
[gpg]
	program = c:\\Program Files (x86)\\GnuPG\\bin\\gpg.exe
[credential]
	helper = wincred
[core]
	autocrlf = true
	symlinks = true
	sshCommand = 'C:\\Windows\\System32\\OpenSSH\\ssh.exe'
	editor = \"C:/Program Files/Microsoft VS Code/code.exe\" --wait
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[merge]
	tool = vscode
[diff]
	guitool = vscode
[pull]
	rebase = false
[fetch]
	prune = false
[rebase]
	autoStash = false
[difftool "vscode"]
	path = C:/Program Files/Microsoft VS Code/code.exe
	cmd = \"C:/Program Files/Microsoft VS Code/code.exe\" --wait --diff \"$LOCAL\" \"$REMOTE\"
[mergetool "vscode"]
	path = C:/Program Files/Microsoft VS Code/code.exe
	cmd = "\"C:/Program Files/Microsoft VS Code/code.exe\" --wait \"$MERGED\" "
[alias]
	clone = 'clone --recursive'
	lgfull = log --oneline --decorate --color --graph
	lg = log --oneline --decorate --color
